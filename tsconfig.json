{
    "compilerOptions": {
      "module": "commonjs",
      "declaration": true,
      "removeComments": true,
      "esModuleInterop": true,
      "target": "es6",
      "noImplicitAny": false,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "sourceMap": true,
      "strict": true,
      "outDir": "./dist",
      "baseUrl": "./",
      "paths": {
        "*": ["node_modules/*"]
      }
    },
    "lib": ["es2015"]
  }